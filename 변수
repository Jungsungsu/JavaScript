<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=<device-width>, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        // 기본 var 변수 키워드 재선언 가능, 재할당 가능
        // 재선언
        var num = 3;
        console.log(num);
        var num = 5;
        console.log(num);
        // 재할당
        var num2 = 10;
        num2 = 8;
        console.log(num2);

        // let 키워드 재선언 불가능, 재할당 가능
        let name ="정성수";
        name = "최성우";

        // const 키워드 (상수) 재선언, 재할당 불가능 함수제작
        const maxLevel = 200;

    </script>
</body>
</html>
